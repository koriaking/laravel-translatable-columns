{
    "name": "koria/laravel-translatable-columns",
    "description": "A trait to make an Eloquent model hold translations",
    "keywords": [
        "laravel-translatable-columns",
        "laravel-translatable",
        "translate",
        "eloquent",
        "model",
        "i8n",
        "multilingual"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yevhen Koriakin",
            "email": "yevhen.koriakin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/database": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Koria\\Translatable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Koria\\Translatable\\Test\\": "tests"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Koria\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
